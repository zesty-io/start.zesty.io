{
  "name": "start-zesty",
  "version": "1.0.0",
  "description": "Application for onboarding users to Zesty.io",
  "main": "webpack.config.js",
  "scripts": {
    "serve": "serve ./build -n -l 6006 --single",
    "start": "webpack-dev-server --env NODE_ENV=stage",
    "build:dev": "./node_modules/.bin/webpack --env NODE_ENV=development",
    "build:stage": "./node_modules/.bin/webpack --mode=production --env NODE_ENV=stage",
    "build:prod": "./node_modules/.bin/webpack --mode=production --env NODE_ENV=production",
    "test:dev": "cypress open",
    "test": "cypress run --config video=false",
    "release:stage": "npm test && zesty deploy stage .",
    "release:prod": "npm test && zesty deploy prod .",
    "cleanup": "echo 'All done'."
  },
  "author": "Zesty.io<developers@zesty.io>",
  "contributors": [
    "Joshua Hanson<joshua.hanson@zesty.io>",
    "Hector Sanchez<hector.sanchez@zesty.io>",
    "Stuart Runyan<stuart@zesty.io>"
  ],
  "license": "All Rights Reserved",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.0",
    "@sentry/react": "10.19.0",
    "@sentry/tracing": "7.120.4",
    "@zesty-io/core": "1.10.0",
    "classnames": "2.5.1",
    "core-js": "^3.6.5",
    "growl": "^1.10.5",
    "html-webpack-plugin": "5.6.4",
    "js-cookie": "3.0.5",
    "lodash.debounce": "4.0.8",
    "qs": "6.14.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-json-view": "^1.19.1",
    "react-redux": "7.1.0",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "redux": "5.0.1",
    "redux-thunk": "3.1.0",
    "regenerator-runtime": "0.14.1"
  },
  "devDependencies": {
    "@babel/core": "7.28.4",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/preset-env": "7.28.3",
    "@babel/preset-react": "7.27.1",
    "babel-cli": "6.26.0",
    "babel-loader": "10.0.0",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "13.0.1",
    "cross-env": "10.1.0",
    "css-loader": "7.1.2",
    "cypress": "15.4.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "husky": "9.1.7",
    "less": "4.4.2",
    "less-loader": "12.3.0",
    "mini-css-extract-plugin": "2.9.4",
    "mkdirp": "3.0.1",
    "prettier": "3.6.2",
    "pretty-quick": "4.2.2",
    "redux-logger": "3.0.6",
    "webpack": "5.102.1",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.2"
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "singleQuote": true,
    "tabWidth": 2,
    "semi": false
  },
  "false": {},
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "volta": {
    "node": "22.20.0"
  }
